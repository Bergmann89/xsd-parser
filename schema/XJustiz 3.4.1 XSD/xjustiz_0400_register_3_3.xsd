                              <xs:element name="erstelltAm" minOccurs="0" type="xs:date"/>
                              <xs:element name="erstelltZumStichtag" minOccurs="0" type="xs:date"/>
                              <xs:element name="freigabeVon" minOccurs="0" type="dinspec91379:datatypeA"/>
                              <xs:element name="freigabeVonStammakte" minOccurs="0" type="xs:date"/>
                              <xs:element name="freigabeVonVorband" minOccurs="0" type="xs:date"/>
                              <xs:element name="gegenstandDerEinreichung"
                                          minOccurs="0"
                                          type="dinspec91379:datatypeC"/>
                              <xs:element name="loeschungssperre" minOccurs="0" type="xs:boolean"/>
                              <xs:element name="merkmalAktuell" minOccurs="0" type="xs:boolean"/>
                              <xs:element name="sperrkennzeichen" minOccurs="0" type="xs:boolean"/>
                              <xs:element name="urkundenNummer" minOccurs="0" type="dinspec91379:datatypeC"/>
                              <xs:element name="urspruenglichesEingangsDatum" minOccurs="0" type="xs:date"/>
                              <xs:element name="vorgangsnummer" minOccurs="0" type="xs:integer"/>
                              <xs:element name="webfreigabeDatum" minOccurs="0" type="xs:date"/>
                              <xs:element name="zustellDatum" minOccurs="0" type="xs:date"/>
                           </xs:sequence>
                        </xs:complexType>
                     </xs:element>
                     <xs:element name="datumDerWirksamkeit"
                                 minOccurs="0"
                                 type="tns:Type.GDS.Datumsangabe">
                        <xs:annotation>
                           <xs:documentation>Effective_Date</xs:documentation>
                        </xs:annotation>
                     </xs:element>
                     <xs:element name="adresseEndempfaengerHandelsregister"
                                 minOccurs="0"
                                 type="dinspec91379:datatypeE">
                        <xs:annotation>
                           <xs:documentation>ausländische Registerbehörde</xs:documentation>
                        </xs:annotation>
                     </xs:element>
                     <xs:element name="auswahl_artZweigniederlassung"
                                 minOccurs="0"
                                 maxOccurs="unbounded">
                        <xs:complexType>
                           <xs:complexContent>
                              <xs:extension base="tns:Type.GDS.Ref.Rollennummer">
                                 <xs:choice>
                                    <xs:element name="gleicheFirma"
                                                fixed="gleiche Firma"
                                                type="dinspec91379:datatypeC"/>
                                    <xs:element name="gleicheFirmaMitZusatz"
                                                fixed="gleiche Firma mit Zusatz"
                                                type="dinspec91379:datatypeC"/>
                                    <xs:element name="abweichendeFirma"
                                                fixed="abweichende Firma"
                                                type="dinspec91379:datatypeC"/>
                                 </xs:choice>
                              </xs:extension>
                           </xs:complexContent>
                        </xs:complexType>
                     </xs:element>
                     <xs:element name="auswahl_vertragsart" minOccurs="0">
                        <xs:complexType>
                           <xs:choice>
                              <xs:element name="vertragsart" type="tns:Code.REG.Vertragsart.Typ3"/>
                              <xs:element name="vertragsartFreitext" type="dinspec91379:datatypeC"/>
                           </xs:choice>
                        </xs:complexType>
                     </xs:element>
                  </xs:sequence>
                  <xs:attribute name="fachdatenRegisterVersion"
                                type="dinspec91379:datatypeC"
                                use="required"
                                fixed="3.3">
                     <xs:annotation>
                        <xs:documentation>Dieses Attribut wird verwendet, um sicherzustellen, dass jedes Instanzdokument gegen die "richtige" Version des XML-Schemas geprüft wird. Angegeben werden hier nur die beiden ersten Stellen der Versionsnummer. Dies ermöglicht es, an einzelnen Dateien Fehlerbereinigungen und dergleichen vorzunehmen, ohne dass die anderen Bestandteile von XJustiz geändert werden müssen.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
               </xs:complexType>
            </xs:element>
         </xs:sequence>
      </xs:complexType>
      <xs:key name="schluessel_beteiligter_beteiligtennummer">
         <xs:annotation>
            <xs:documentation>Eindeutiger Schlüssel ist hier die Beteiligtennummer eines Beteiligten.</xs:documentation>
         </xs:annotation>
         <xs:selector xpath="tns:grunddaten/tns:verfahrensdaten/tns:beteiligung/tns:beteiligter"/>
         <xs:field xpath="tns:beteiligtennummer"/>
      </xs:key>
      <xs:key name="schluessel_beteiligter_rollennummer">
         <xs:annotation>
            <xs:documentation>Eindeutiger Schlüssel ist hier die Rollennummer eines Beteiligten.</xs:documentation>
         </xs:annotation>
         <xs:selector xpath="tns:grunddaten/tns:verfahrensdaten/tns:beteiligung/tns:rolle"/>
         <xs:field xpath="tns:rollennummer"/>
      </xs:key>
      <xs:key name="schluessel_instanz">
         <xs:annotation>
            <xs:documentation>Eindeutiger Schlüssel ist hier die Instanznummer einer Instanz.</xs:documentation>
         </xs:annotation>
         <xs:selector xpath="tns:grunddaten/tns:verfahrensdaten/tns:instanzdaten"/>
         <xs:field xpath="tns:instanznummer"/>
