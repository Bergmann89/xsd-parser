         <xs:selector xpath=".//*"/>
         <xs:field xpath="tns:ref.instanznummer"/>
      </xs:keyref>
      <xs:keyref name="ref_rollennummer" refer="tns:schluessel_beteiligter_rollennummer">
         <xs:selector xpath=".//*"/>
         <xs:field xpath="tns:ref.rollennummer"/>
      </xs:keyref>
   </xs:element>
</xs:schema>
