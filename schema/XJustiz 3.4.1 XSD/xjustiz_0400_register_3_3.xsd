                           <xs:sequence>
                              <xs:element name="ref.dokumentID" type="tns:Type.GDS.Ref.SGO">
                                 <xs:annotation>
                                    <xs:documentation>Hier wird auf ein Dokument über die Dokument_ID dieses Dokuments Bezug genommen.</xs:documentation>
                                 </xs:annotation>
                              </xs:element>
                              <xs:element name="stichtag" minOccurs="0" type="xs:date">
                                 <xs:annotation>
                                    <xs:documentation>Hier wird der Stichtag des Dokuments angegeben z.B. Bilanzstichtag oder bei Gesellschafterlisten wichtig.</xs:documentation>
                                 </xs:annotation>
                              </xs:element>
                              <xs:element name="sprache" minOccurs="0" type="tns:Code.GDS.Sprachen"/>
