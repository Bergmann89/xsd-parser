                              <xs:element name="bemerkung" minOccurs="0" type="dinspec91379:datatypeC"/>
                              <xs:element name="bezug" minOccurs="0" type="dinspec91379:datatypeC"/>
                              <xs:element name="doppelstueck" minOccurs="0" type="xs:boolean"/>
