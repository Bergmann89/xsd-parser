<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:tns="http://www.xjustiz.de"
           xmlns:dinspec91379="urn:xoev-de:kosit:xoev:datentyp:din-spec-91379_2019-03"
           xmlns:xoev-code="http://xoev.de/schemata/code/1_0"
           targetNamespace="http://www.xjustiz.de"
           version="3.4.0"
           elementFormDefault="qualified"
           attributeFormDefault="unqualified">
   <xs:annotation>
      <xs:appinfo>
         <standard>
            <nameLang>Strukturierte Fachdaten für die Kommunikation im elektronischen Rechtsverkehr</nameLang>
            <nameKurz>XJustiz</nameKurz>
            <nameTechnisch>xjustiz</nameTechnisch>
            <kennung>urn:xoev-de:blk-ag-it-standards:standard:xjustiz</kennung>
            <beschreibung>XJustiz ist der bundesweit einheitliche Standard für den Austausch strukturierter elektronischer Informationen mit der Justiz.</beschreibung>
         </standard>
         <versionStandard>
            <version>3.4.1</version>
            <beschreibung>XJustiz beschreibt ein standardisiertes Datenaustauschformat für die elektronische Kommunikation innerhalb und mit der Justizverwaltung.</beschreibung>
            <versionXOEVProfil>1.7.2</versionXOEVProfil>
            <versionXOEVHandbuch>2.3.1</versionXOEVHandbuch>
            <versionXGenerator>3.0.1</versionXGenerator>
            <versionModellierungswerkzeug>19.0 SP3</versionModellierungswerkzeug>
            <nameModellierungswerkzeug>MagicDraw</nameModellierungswerkzeug>
         </versionStandard>
      </xs:appinfo>
   </xs:annotation>
   <xs:include schemaLocation="xjustiz_0005_nachrichten_3_0.xsd"/>
   <xs:include schemaLocation="xjustiz_0510_cl_straf_3_3.xsd"/>
   <xs:include schemaLocation="xjustiz_0520_cl_personenwerte_3_2.xsd"/>
   <xs:include schemaLocation="xjustiz_0530_cl_instanzwerte_3_1.xsd"/>
   <xs:include schemaLocation="xjustiz_0540_cl_kfzkennzeichen_3_1.xsd"/>
   <xs:include schemaLocation="xjustiz_0550_cl_nachrichten_3_3.xsd"/>
   <xs:import schemaLocation="din-91379-datatypes.xsd"
              namespace="urn:xoev-de:kosit:xoev:datentyp:din-spec-91379_2019-03"/>
   <xs:import schemaLocation="xoev-code.xsd"
              namespace="http://xoev.de/schemata/code/1_0"/>
   <xs:complexType name="Type.STRAF.Anordnungsinhalt">
      <xs:annotation>
         <xs:documentation>Hier kann es sich um eine Geldanordnung oder Sonstige Anordnung handeln.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="nummer" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Diese Nummer ist notwendig, um vom Element Haftvollzug auf eine Anordnung zu verweisen, auf der die Haft zugrunde liegt.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="tatbestaendeEinzelstrafen"
                     minOccurs="0"
                     type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Hier kann der Paragraph des Tatbestandes angegeben werden.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="tateinheitMitNichtregisterpflichtigenTaten"
                     minOccurs="0"
                     type="xs:boolean">
            <xs:annotation>
               <xs:documentation>Liegt eine Tateinheit nach § 52 StGB vor, die sich aus registerpflichtigen und nichtregisterpflichtigen Taten zusammensetzt, ist dies hier anzugeben. Das Element bezieht sich auf die Eintragung von Tatbeständen in das Wettbewerbsregister und beziehen sich auf die registerpflichtigen Taten nach § 2 WRegG.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:choice>
            <xs:element name="geldanordnung">
               <xs:annotation>
                  <xs:documentation>Gemeint ist jede Art von Sanktion, bei der Geld zu zahlen ist. Alle Formen der Verurteilung zu einer Geldzahlung.</xs:documentation>
               </xs:annotation>
               <xs:complexType>
                  <xs:sequence>
                     <xs:element name="anordnungsart"
                                 minOccurs="0"
                                 type="tns:Code.STRAF.Geldanordnungsart.Typ3">
                        <xs:annotation>
                           <xs:documentation>Für die Inhalte dieses Elementes wird die Codeliste STRAF.Geldanordnungsart verwendet. Mögliche Werte sind hier z.B. Geldstrafe, Geldbuße...</xs:documentation>
                        </xs:annotation>
                     </xs:element>
                     <xs:element name="betrag" minOccurs="0">
                        <xs:complexType>
                           <xs:sequence>
                              <xs:element name="anzahlTagessaetze"
                                          minOccurs="0"
                                          type="dinspec91379:datatypeC"/>
                              <xs:element name="hoeheTagessatz" minOccurs="0" type="tns:Type.GDS.Geldbetrag"/>
                              <xs:element name="gesamtbetrag" type="tns:Type.GDS.Geldbetrag"/>
                              <xs:element name="stundensatz" minOccurs="0" type="dinspec91379:datatypeC">
                                 <xs:annotation>
                                    <xs:documentation>Bei Ableistung durch gemeinnützige Arbeit: Wie viele Arbeitsstunden entsprechen einem Tagessatz?</xs:documentation>
                                 </xs:annotation>
                              </xs:element>
                              <xs:element name="empfaenger" minOccurs="0" type="dinspec91379:datatypeD">
                                 <xs:annotation>
                                    <xs:documentation>Dieses Element ist neu mitaufznehmen. Es kann einen Verweis auf einen Beteiligten im Grunddatensatz enthalten.</xs:documentation>
                                 </xs:annotation>
                              </xs:element>
                           </xs:sequence>
                        </xs:complexType>
                     </xs:element>
                     <xs:element name="faelligkeit" minOccurs="0" type="dinspec91379:datatypeC"/>
                     <xs:element name="strafvorbehalt">
                        <xs:complexType>
                           <xs:sequence>
                              <xs:element name="strafvorbehalt" type="xs:boolean">
                                 <xs:annotation>
                                    <xs:documentation>Dieses Element enthält einen Ja/Nein-Wert. Ist dieser Wert auf "Ja" gesetzt, dann werden weitere Einzelheiten im Element "erlaeuterung" mitgeteilt.</xs:documentation>
                                 </xs:annotation>
                              </xs:element>
                              <xs:element name="erlaeuterung" minOccurs="0" type="dinspec91379:datatypeC">
                                 <xs:annotation>
                                    <xs:documentation>Hier werden weitere Einzelheiten zum Strafvorbehalt angegeben.</xs:documentation>
                                 </xs:annotation>
                              </xs:element>
                           </xs:sequence>
                        </xs:complexType>
                     </xs:element>
                     <xs:element name="stundung" minOccurs="0">
                        <xs:complexType>
                           <xs:sequence>
                              <xs:element name="ratenanzahl" minOccurs="0" type="dinspec91379:datatypeC"/>
                              <xs:element name="ratenbetrag" minOccurs="0" type="tns:Type.GDS.Geldbetrag">
                                 <xs:annotation>
                                    <xs:documentation>Hier wird der Betrag der regelmäßig zu erbringenden Raten angegeben</xs:documentation>
                                 </xs:annotation>
                              </xs:element>
                              <xs:element name="zahlungsbeginn" minOccurs="0" type="xs:date"/>
                              <xs:element name="periode" minOccurs="0" type="dinspec91379:datatypeC">
                                 <xs:annotation>
                                    <xs:documentation>monatlich, 1/4 jährlich...</xs:documentation>
                                 </xs:annotation>
                              </xs:element>
                           </xs:sequence>
                        </xs:complexType>
                     </xs:element>
                     <xs:element name="zahlung"
                                 minOccurs="0"
                                 maxOccurs="unbounded"
                                 type="tns:Type.STRAF.Zahlung">
                        <xs:annotation>
                           <xs:documentation>Hier sind Zahlungen zu einer Geldanordnung zu erfassen. Dies können auch Ratenzahlungen oder Rücküberweisungen sein.</xs:documentation>
                        </xs:annotation>
                     </xs:element>
                  </xs:sequence>
               </xs:complexType>
            </xs:element>
            <xs:element name="sonstigeAnordnung">
               <xs:annotation>
                  <xs:documentation>Anderweitige Anordnungen</xs:documentation>
               </xs:annotation>
               <xs:complexType>
                  <xs:sequence>
                     <xs:element name="anordnungsart" type="tns:Code.STRAF.Anordnungsart.Typ3">
                        <xs:annotation>
                           <xs:documentation>Für die Inhalte dieses Elementes wird eine Codeliste WL_Anordnungsart verwendet. Mögliche Werte sind hier z.B. Freiheitsstrafe, Entzug der Fahrerlaubnis, Sperrfrist für die Wiedererteilung</xs:documentation>
                        </xs:annotation>
                     </xs:element>
                     <xs:element name="grund" minOccurs="0" type="dinspec91379:datatypeC">
                        <xs:annotation>
                           <xs:documentation>Ein Beispiel für den Grund einer Anordnung ist Fluchtgefahr bei einer U-Haftanordnung.</xs:documentation>
                        </xs:annotation>
                     </xs:element>
                     <xs:element name="beschreibung" minOccurs="0" type="dinspec91379:datatypeC">
                        <xs:annotation>
                           <xs:documentation>Optionaler Freitext zu näheren Beschreibung der Sanktion (z.B. Entziehung der Fahrerlaubnis nur für bestimmte Erlaubnisklassen)</xs:documentation>
                        </xs:annotation>
                     </xs:element>
                     <xs:element name="ref.beweismittel"
                                 minOccurs="0"
                                 maxOccurs="unbounded"
                                 type="dinspec91379:datatypeC">
                        <xs:annotation>
                           <xs:documentation>Optionaler Verweis auf ein Beweismittel.</xs:documentation>
                        </xs:annotation>
                     </xs:element>
                     <xs:element name="ref.asservate"
                                 minOccurs="0"
                                 maxOccurs="unbounded"
                                 type="dinspec91379:datatypeC">
                        <xs:annotation>
                           <xs:documentation>Optionaler Verweis auf Asservate</xs:documentation>
                        </xs:annotation>
                     </xs:element>
                     <xs:element name="dauer" minOccurs="0" type="tns:Type.STRAF.Dauer"/>
                     <xs:element name="faelligkeit" minOccurs="0" type="dinspec91379:datatypeC"/>
                     <xs:element name="beginn" minOccurs="0" type="dinspec91379:datatypeC"/>
                     <xs:element name="ende" minOccurs="0" type="dinspec91379:datatypeC"/>
                     <xs:element name="anrechnung" minOccurs="0" type="dinspec91379:datatypeC">
                        <xs:annotation>
                           <xs:documentation>Anordnungen des Gerichts über die Anrechnung anderweitigen Freiheitsentzuges, z.B. U-Haft im Ausland.</xs:documentation>
                        </xs:annotation>
                     </xs:element>
                     <xs:element name="bewaehrungshelfer"
                                 minOccurs="0"
                                 type="tns:Type.GDS.Ref.Rollennummer">
                        <xs:annotation>
                           <xs:documentation>Verweis auf Beteiligten. Das Element kann natürlich auch für vergleichbare Personen verwendet werden, wie z.B. Betreuungshelfer nach § 10 Abs. 1 Nr. 5 JGG</xs:documentation>
                        </xs:annotation>
                     </xs:element>
                     <xs:element name="arbeitgeber"
                                 minOccurs="0"
                                 type="tns:Type.GDS.Ref.Rollennummer">
                        <xs:annotation>
                           <xs:documentation>Dieses Element beinhaltet einen Verweis auf einen Beteiligten im Grunddatensatz</xs:documentation>
                        </xs:annotation>
                     </xs:element>
                  </xs:sequence>
               </xs:complexType>
            </xs:element>
         </xs:choice>
         <xs:element name="vollstreckungsverjaehrung" minOccurs="0" type="xs:date">
            <xs:annotation>
               <xs:documentation>Das Datum, bis zu dem die Entscheidung / Anordnung vollstreckt werden darf.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.Asservate">
      <xs:sequence>
         <xs:element name="nummer" minOccurs="0" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Da von anderen Elementen auf schon erfasste Asservate verwiesen werden soll, ist ein eindeutiges Nummern-Feld für das Element 'asservate' notwendig.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="asservatenID" type="dinspec91379:datatypeC"/>
         <xs:element name="auswahl_asservatmitteilung" minOccurs="0">
            <xs:complexType>
               <xs:choice>
                  <xs:element name="statusmitteilung"
                              type="tns:Code.STRAF.Asservat.Statusmitteilung.Typ3"/>
                  <xs:element name="auftrag" type="tns:Code.STRAF.Asservat.Auftrag.Typ3"/>
               </xs:choice>
            </xs:complexType>
         </xs:element>
         <xs:element name="grund" minOccurs="0" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Grund für die Nichtübernahme oder veränderte Übernahme des Asservates z.B. fehlende Anlieferung, falsche Menge etc.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="gegenstandsart" minOccurs="0" type="dinspec91379:datatypeC"/>
         <xs:element name="aufbewahrungsbehoerde"
                     minOccurs="0"
                     type="tns:Type.GDS.Behoerde"/>
         <xs:element name="gefahrgut" minOccurs="0" type="xs:boolean"/>
         <xs:element name="lagerhinweis" minOccurs="0" type="dinspec91379:datatypeC"/>
         <xs:element name="bezeichnung" minOccurs="0" type="dinspec91379:datatypeC"/>
         <xs:element name="menge" minOccurs="0" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Hier kann die Menge als Freitext erfasst werden.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="einheit" minOccurs="0" type="dinspec91379:datatypeC"/>
         <xs:element name="herkunft" minOccurs="0" maxOccurs="unbounded">
            <xs:complexType>
               <xs:sequence>
                  <xs:element name="person" type="tns:Type.GDS.Ref.Rollennummer">
                     <xs:annotation>
                        <xs:documentation>Die beteiligte Person wird über einen Verweis auf die Rollennummer eines Beteiligten im Grunddatensatz angegeben.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="herkunftsart"
                              minOccurs="0"
                              type="tns:Code.STRAF.Herkunftsart.Typ3"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element name="gruppe" minOccurs="0" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Gemeint ist die Gruppe, unter der das Asservat erfasst wurde, z.B. 8/04</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="laufendeNummer" minOccurs="0" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Die laufende Nummer in der Asservatengruppe, z.B. Nr.4</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="einlagerungsdatum" minOccurs="0" type="xs:date"/>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.BFJ.Ausgangszusatztext">
      <xs:sequence>
         <xs:element name="position" type="xs:positiveInteger">
            <xs:annotation>
               <xs:documentation>Dieses Attribut dient zur Sortierung von Ausgangstexten, die einer bestimmten Entscheidung zugeordnet sind. Es ist eine positive ganze Zahl einzutragen.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ausgangstext" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Dieses Element steht für eine Zusatzinformation zur vorliegenden Entscheidung. Das Element darf maximal 2048 Zeichen lang sein. Es dürfen verwendet werden: Buchstaben, Ziffern, Sonderzeichen (Zwischenraum und die Zeichen: " ' ´ ` ( ) * + , - . / ; = ? §). Gleiche Sonderzeichen dürfen nicht aufeinander folgen.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.BFJ.BZR.Textkennzahl">
      <xs:sequence>
         <xs:element name="textkennzahl" type="tns:Code.STRAF.BFJ.BZR.Textkennzahl.Typ3">
            <xs:annotation>
               <xs:documentation>Dieses Element bezeichnet die verwendete Textkennzahl.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="zusatztext" minOccurs="0" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Dieses Element gibt den zu einer Textkennzahl im Register zu vermerkenden bzw. vermerkten Inhalt wieder, ggf. mit Zusätzen oder Datumsangaben. Das Element darf maximal 2042 Zeichen lang sein. Es dürfen verwendet werden: Buchstaben, Ziffern, Sonderzeichen (Zwischenraum und die Zeichen: " ' ´ ` ( ) * + , - . / ; = ? §). Gleiche Sonderzeichen dürfen nicht aufeinander folgen.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.BFJ.Betrag">
      <xs:sequence>
         <xs:element name="betrag" type="xs:double">
            <xs:annotation>
               <xs:documentation>In diesem Element ist der Betrag in Eurocent bzw. Pfennig anzugeben.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="auswahl_waehrung">
            <xs:annotation>
               <xs:documentation>Dieses Element steht für die Währung, in der der Betrag angegeben ist.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:choice>
                  <xs:element name="eur" type="xs:boolean"/>
                  <xs:element name="dm" type="xs:boolean"/>
               </xs:choice>
            </xs:complexType>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.BFJ.BewaehrungszeitDauer">
      <xs:choice>
         <xs:element name="dauer" type="tns:Type.STRAF.Dauer">
            <xs:annotation>
               <xs:documentation>Dauer der Bewährungszeit als zeitlicher Umfang.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="datum" type="xs:date">
            <xs:annotation>
               <xs:documentation>Dauer der Bewährungszeit, falls dargestellt in der Schreibweise mit Datum. Es ist das Datum einzutragen, an dem die Bewährungszeit endet.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:choice>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.BFJ.DatenRechtswirksamkeit">
      <xs:sequence>
         <xs:element name="datumRechtskraft" minOccurs="0" type="xs:date">
            <xs:annotation>
               <xs:documentation>Datum, an dem die Rechtskraft einer Entscheidung eingetreten ist.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="datumVollziehbarkeit" minOccurs="0" type="xs:date">
            <xs:annotation>
               <xs:documentation>Datum, an dem die Vollziehbarkeit der Verwaltungsentscheidung eingetreten ist.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="datumUnanfechtbar" minOccurs="0" type="xs:date">
            <xs:annotation>
               <xs:documentation>Datum, an dem die Unanfechtbarkeit der Verwaltungsentscheidung eingetreten ist.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="datumVerzichtRechtswirksamkeit" minOccurs="0" type="xs:date">
            <xs:annotation>
               <xs:documentation>Datum, an dem die Rechtswirksamkeit eines Verzichts eingetreten ist.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.BFJ.Fahrerlaubnis">
      <xs:choice>
         <xs:element name="fahrerlaubnissperreBis" type="xs:date">
            <xs:annotation>
               <xs:documentation>Falls eine befristete Sperre für die Neuerteilung der Fahrerlaubnis vorliegt, ist hier das Datum einzutragen, an dem die Sperre endet.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="fahrerlaubnissperreFuerImmer" type="xs:boolean">
            <xs:annotation>
               <xs:documentation>Falls die Sperre für die Neuerteilung der Fahrerlaubnis für immer gilt, ist dieses Element zu übermitteln.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:choice>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.BFJ.Freiheitsentziehung">
      <xs:sequence>
         <xs:element name="art" type="tns:Code.STRAF.BFJ.BZR.FreiheitsentziehungArt.Typ3">
            <xs:annotation>
               <xs:documentation>Art der Freiheitsentziehung: Die in der Entscheidung ausgesprochene Art der Freiheitsentziehung gemäß Codeliste Freiheitsentziehung, beispielsweise Jugendstrafe, Freiheitsstrafe etc.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="auswahl_dauer">
            <xs:annotation>
               <xs:documentation>Dauer der Freiheitsentziehung.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:choice>
                  <xs:element name="dauer" type="tns:Type.STRAF.Dauer">
                     <xs:annotation>
                        <xs:documentation>Dauer der Freiheitsentziehung.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="lebenslang" type="xs:boolean">
                     <xs:annotation>
                        <xs:documentation>Falls eine lebenslange Freiheitsstrafe verhängt wurde, ist dieses Element zu übermitteln.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:choice>
            </xs:complexType>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.BFJ.GZR.Rechtsvorschrift">
      <xs:sequence>
         <xs:element name="bezeichnungLang" minOccurs="0" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Deskriptive Bezeichnung der Rechtsvorschrift (muss angegeben werden, falls kein Code angegeben ist. Dieses Element darf maximal 2048 Zeichen lang sein.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="bezeichnungCode"
                     minOccurs="0"
                     type="tns:Code.STRAF.BFJ.GZR.Rechtsvorschriften.Typ3">
            <xs:annotation>
               <xs:documentation>Identifizierung der Rechtsvorschrift über einen Code (muss angegeben werden, falls keine deskriptive Bezeichnung angegeben ist.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="paragraph" minOccurs="0" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Angabe des Paragraphen zur Rechtsvorschrift. Dieses Element darf maximal 5 Zeichen lang sein.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="artikel" minOccurs="0" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Angabe zur Rechtsvorschrift: Artikel Dieses Element darf maximal 5 Zeichen lang sein.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="absatz" minOccurs="0" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Angabe zur Rechtsvorschrift: Absatz Dieses Element darf maximal 2 Zeichen lang sein.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="nummer" minOccurs="0" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Angabe zur Rechtsvorschrift: Nummer Dieses Element darf maximal 5 Zeichen lang sein.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="buchstabe" minOccurs="0" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Angabe zur Rechtsvorschrift: Buchstabe Dieses Element darf maximal 1 Zeichen lang sein.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="satz" minOccurs="0" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Angabe zur Rechtsvorschrift: Satz Dieses Element darf maximal 1 Zeichen lang sein.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="halbsatz" minOccurs="0" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Angabe zur Rechtsvorschrift: Halbsatz Dieses Element darf maximal 1 Zeichen lang sein.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="alternative" minOccurs="0" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Angabe zur Rechtsvorschrift: Alternative Dieses Element darf maximal 3 Zeichen lang sein.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.BFJ.GZR.Textkennzahl">
      <xs:sequence>
         <xs:element name="textkennzahl" type="tns:Code.STRAF.BFJ.GZR.Textkennzahl.Typ3">
            <xs:annotation>
               <xs:documentation>Dieses Element bezeichnet die verwendete Textkennzahl.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="zusatztext" minOccurs="0" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Dieses Element gibt den ggf. zu einer Textkennzahl im Register zu vermerkenden bzw. vermerkten Inhalt wieder, ggf. mit Datumsangaben. Das Element darf maximal 2042 Zeichen lang sein. Es dürfen verwendet werden: Buchstaben, Ziffern, Sonderzei chen (Zwischenraum und die Zeichen: " ' ´ ` ( ) * + , - . / ; = ? §). Gleiche Sonderzeichen dürfen nicht aufeinander folgen.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.BFJ.Geldstrafe">
      <xs:sequence>
         <xs:element name="anzahlTagessaetze" type="xs:positiveInteger">
            <xs:annotation>
               <xs:documentation>Hier ist Anzahl der verhängten Tagessätze einzutragen, falls eine Geldstrafe vorliegt.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="hoeheTagessaetze" type="tns:Type.STRAF.BFJ.Betrag">
            <xs:annotation>
               <xs:documentation>Höhe des Tagessatzes einer Geldstrafe.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.BFJ.Ordnungsdaten">
      <xs:sequence>
         <xs:element name="entscheidungsdatum" type="xs:date">
            <xs:annotation>
               <xs:documentation>Datum der ersten Entscheidung des ersten Rechtszugs. Ist gegen einen Strafbefehl Einspruch eingelegt worden, ist die auf den Einspruch ergangene Entscheidung erste Entscheidung des ersten Rechtszugs, außer wenn der Einspruch verworfen wurde.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="aktenzeichen" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Bezeichnung des Vorgangs (Aktenzeichen, Geschäftszeichen), unter dem die Entscheidung getroffen bzw. der Verzicht erklärt wurde. Dieses Element darf maximal 100 Zeichen lang sein.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="behoerdeErkennend">
            <xs:annotation>
               <xs:documentation>Bezeichnung der Stelle, bei der die Entscheidung getroffen bzw. der Verzicht erklärt wurde.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element name="kennzeichnung" minOccurs="0" type="tns:Type.GDS.Behoerde">
                     <xs:annotation>
                        <xs:documentation>Hier ist die XJustiz-ID des Gerichts aus der Codeliste Code.GDS.Gerichte aufzuführen. Sollte für die erkennde Behörde keine XJustiz-ID vorhanden sein, soll der Name der Behörde in dem Element "behoerdenname" aufgeführt werden.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="behoerdenname" type="dinspec91379:datatypeB">
                     <xs:annotation>
                        <xs:documentation>Namen der erkennenden (entscheidenden) Stelle.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="anschrift" minOccurs="0" type="tns:Type.GDS.Anschrift">
                     <xs:annotation>
                        <xs:documentation>Anschrift der erkennenden Stelle</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element name="laufendeNummer" minOccurs="0" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Im Register geführte laufende Nummer der Entscheidung zu der gegebenen Person.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.BFJ.Statistik">
      <xs:sequence>
         <xs:element name="gewerbeart" type="tns:Code.STRAF.BFJ.GZR.Gewerbeart.Typ3">
            <xs:annotation>
               <xs:documentation>Angabe der Art des Gewerbes</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="gewerbeschluessel"
                     type="tns:Code.STRAF.BFJ.GZR.Gewerbeschluessel.Typ3">
            <xs:annotation>
               <xs:documentation>Angabe des Gewerbeschlüssels.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.BFJ.Straftat">
      <xs:sequence>
         <xs:element name="datumDerTat" minOccurs="0" type="tns:Type.GDS.Datumsangabe">
            <xs:annotation>
               <xs:documentation>Datum der Tat. Bei mehreren Straftaten: Datum der letzten Tat. Bei fortgesetzter Handlung: Datum des Endes der Tathandlung. Wenn in diesem Element keine Angabe gemacht wird, ist der Tatzeitpunkt nicht bekannt.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="tatbezeichnung" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Rechtliche Bezeichnung der Tat, wie sie sich aus der Urteilsformel ergibt, ggf. mit Angaben zu Täterschaft und Teilnahme sowie zum Versuch. Dieses Element darf maximal 2048 Zeichen lang sein.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="angewendeteRechtsvorschriften" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Die nach § 260 Abs. 5 StPO nach der Urteilsformel aufgeführten Vorschriften. Die Bezeichnung des angewendeten Gesetzes ist vorangestellt. Die einzelne Vorschrift beginnt mit dem Zeichen § bzw. der Angabe "Artikel". Mehrere Vorschriften sind jeweils durch Kommata getrennt. Beispiel: StGB §§ 211, 22, 23. Dieses Element darf maximal 2048 Zeichen lang sein.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.BFJ.UebermittelndeStelle">
      <xs:sequence>
         <xs:element name="sender" type="tns:Code.STRAF.BFJ.UebermittelndeStelle.Typ3">
            <xs:annotation>
               <xs:documentation>Der Sender der Nachricht auf Ebene der Transportschicht.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="empfaenger"
                     minOccurs="0"
                     type="tns:Code.STRAF.BFJ.UebermittelndeStelle.Typ3">
            <xs:annotation>
               <xs:documentation>Der Empfänger der Nachricht auf Ebene der Transportschicht.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.BFJ.Verwendungszweck">
      <xs:annotation>
         <xs:documentation>Dieser Datentyp steht für den Zweck, zu dem eine Auskunft benötigt wird. Dieser ist von der anfragenden Stelle bei der Anfrage anzugeben. Stellen erhalten nur für die im Gesetz (BZRG, GewO) vorgesehenen Zwecke eine Auskunft aus einem Register des BfJ.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="verwendungszweckCode"
                     type="tns:Code.STRAF.BFJ.VerwendungszweckAuskunft.Typ3">
            <xs:annotation>
               <xs:documentation>Dieses Element steht für den Zweck, zu dem die anfragende Stelle die Auskunft benötigt. Dieser ist von der anfragenden Stelle bei der Anfrage anzugeben. Stellen erhalten nur für die im Gesetz (BZRG, GewO) vorgesehenen Zwecke eine Auskunft aus einem Register des BfJ.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="zusatz" minOccurs="0" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Für nähere Erläuterungen zum Zweck, für den die Auskunft benötigt wird, kann hier ein Freitext eingefügt werden. Der Freitext darf maximal 44 Zeichen lang sein. Alle Zusätze werden im BfJ intellektuell geprüft, wodurch sich die Erteilung der Auskunft verzögert. Daher sollte in der Regel auf Zusätze verzichtet werden. Falls im Element verwendungszweck der Verwendungszweck "U99" ausgewählt wird, muss zwingend eine Angabe im Element zusatz übermittelt werden.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.BFJ.WeitereAngabenBeteiligter">
      <xs:sequence>
         <xs:element name="ref.rollennummer"
                     minOccurs="0"
                     type="tns:Type.GDS.Ref.Rollennummer">
            <xs:annotation>
               <xs:documentation>Die Rollennummer definiert, welcher konkreten Rolle die Beteiligtendaten im jeweiligen Kontext zuzuordnen sind.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="artDerAuskunftsdaten"
                     minOccurs="0"
                     type="tns:Code.STRAF.BFJ.ArtDerAuskunftsdaten.Typ3">
            <xs:annotation>
               <xs:documentation>Über diesen Wert wird definiert, ob es sich bei den wiedergegebenen Beteiligtendaten um die Beteiligtendaten der Anfrage oder um die führenden Beteiligtendaten des Registers oder um die von diesen Daten abweichenden Beteiligtendaten des Registers handelt.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="dnummer" minOccurs="0" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Die DNummer (daktyloskopische Referenz-Nummer) referenziert auf alle beim BKA abgespeicherten Finger- und/oder Handflächenabdrücke zu einer Person. Sie besteht aus einem Buchstaben gefolgt von 12 Ziffern, mithin aus 13 Zeichen. Sie ist anzugeben, wenn es sich bei der betroffenen Person um einen Drittstaatsangehörigen (also um einen Staatsangehörigen eines Nicht-EU-Staates), einen Staatenlosen oder eine Person mit ungeklärter Staatsangehörigkeit handelt.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="anschriftUnstrukturiert"
                     minOccurs="0"
                     type="dinspec91379:datatypeB">
            <xs:annotation>
               <xs:documentation>An dieser Stelle können die Anschriftsinformationen unstrukturiert eingetragen werden. Die Daten werden als fortlaufende Zeichenkette geschrieben. Eine bestimmte Reihenfolge oder Trennzeichen sind nicht vorgegeben (Beispiel: „53113 Bonn, Adenauerallee“).</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="weitereAngaben"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="dinspec91379:datatypeE">
            <xs:annotation>
               <xs:documentation>Zusätzliche Angaben aus den Daten des ausländischen Registers, z.B. frühere Namen der betroffenen Person.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="geburtsnameZweifelhaft"
                     minOccurs="0"
                     type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Ist in Mitteilungen an BZR oder GZRnat ein Alias-Geburtsname der betroffenen Person enthalten, muss dieses Element übermittelt werden. Es kann entweder ein 'A' oder ein 'B' enthalten, alternative Eintragungen sind nicht möglich. Falls der mitteilenden Stelle bekannt ist, dass der führende Geburtsname zutreffend ist, ist ein 'A' einzutragen. Andernfalls - wenn der mitteilenden Stelle also nicht bekannt ist, ob der führende Geburtsname oder aber ein Alias-Geburtsname zutreffend ist - ein 'B'.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.Beschlagnahme">
      <xs:sequence>
         <xs:element name="person" type="tns:Type.GDS.Ref.Rollennummer">
            <xs:annotation>
               <xs:documentation>Hauptbeteiligte Person im Verfahren</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="datum" type="xs:date"/>
         <xs:element name="fuehrerschein"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="tns:Type.STRAF.Fuehrerschein"/>
         <xs:element name="gegenstand" minOccurs="0" maxOccurs="unbounded">
            <xs:complexType>
               <xs:sequence>
                  <xs:element name="art" type="dinspec91379:datatypeC">
                     <xs:annotation>
                        <xs:documentation>Art des Gegenstandes</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="erlaeuterung" minOccurs="0" type="dinspec91379:datatypeC">
                     <xs:annotation>
                        <xs:documentation>Erläuterung des Gegenstandes</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="betroffener" type="tns:Type.GDS.Ref.Rollennummer">
                     <xs:annotation>
                        <xs:documentation>Verweis auf die Person (über die Rollennummer des Grunddatensatzes), welche vermutlich im Besitz des Gegenstandes ist.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="herkunftsbezeichnung"
                              minOccurs="0"
                              type="dinspec91379:datatypeD">
                     <xs:annotation>
                        <xs:documentation>Erläuterung des Gegenstandes; bei Dokumenten Ausstellungsbehörde; bei Scheckkarte Geldinstitut etc...</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="typ" minOccurs="0" type="dinspec91379:datatypeD">
                     <xs:annotation>
                        <xs:documentation>Typ/Modell/Nennwert</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="kennzeichen" minOccurs="0" type="dinspec91379:datatypeC">
                     <xs:annotation>
                        <xs:documentation>amtliches / Versicherungs-Kennzeichen</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="nationalitaetskennzeichen"
                              minOccurs="0"
                              type="dinspec91379:datatypeC">
                     <xs:annotation>
                        <xs:documentation>Bei Kfz, Kennzeichen und Personaldokumenten immer angeben.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="FIN" minOccurs="0" type="dinspec91379:datatypeC">
                     <xs:annotation>
                        <xs:documentation>Fahrzeugidentifikationsnummer</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="gegenstandsnummer"
                              minOccurs="0"
                              type="dinspec91379:datatypeC"/>
                  <xs:element name="motornummer" minOccurs="0" type="dinspec91379:datatypeC"/>
                  <xs:element name="hinweise" minOccurs="0" type="dinspec91379:datatypeC">
                     <xs:annotation>
                        <xs:documentation>Sachgebundene Hinweise: Sachwertdelikte, Gefahr der Bewaffnung, Explosionsgefahr, Gefährliche Stoffe, Ansteckungsgefahr</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="besondereMerkmale"
                              minOccurs="0"
                              type="dinspec91379:datatypeC">
                     <xs:annotation>
                        <xs:documentation>Bsp. Fahrrad: 18-Gang, 26-Zoll-Reifen, Herren-, Damen-, Rennrad oder Mountainbike ...</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="farbe" minOccurs="0" type="dinspec91379:datatypeC">
                     <xs:annotation>
                        <xs:documentation>Farbe des Gegenstandes</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="materialbezeichnung"
                              minOccurs="0"
                              type="dinspec91379:datatypeC"/>
                  <xs:element name="erlMaterialbezeichnung"
                              minOccurs="0"
                              type="dinspec91379:datatypeC">
                     <xs:annotation>
                        <xs:documentation>erl. Materialbezeichnung SEM</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.Bewaehrung">
      <xs:sequence>
         <xs:element name="bewaehrungsaufsicht" minOccurs="0" type="xs:boolean"/>
         <xs:element name="bewaehrungshelfer"
                     minOccurs="0"
                     type="tns:Type.GDS.Ref.Rollennummer"/>
         <xs:element name="zeitraumBewaehrungshelferunterstellung" minOccurs="0">
            <xs:complexType>
               <xs:sequence>
                  <xs:element name="beginn" minOccurs="0" type="xs:date"/>
                  <xs:element name="ende" minOccurs="0" type="xs:date"/>
                  <xs:element name="dauer" minOccurs="0" type="tns:Type.STRAF.Dauer"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element name="entbindung" minOccurs="0" type="xs:boolean"/>
         <xs:element name="neubestellung" minOccurs="0" type="xs:boolean"/>
         <xs:element name="auflagen" minOccurs="0" maxOccurs="unbounded">
            <xs:complexType>
               <xs:sequence>
                  <xs:element name="auflage" type="tns:Code.STRAF.Auflagen.Typ3"/>
                  <xs:element name="betrag" minOccurs="0" type="tns:Type.GDS.Geldbetrag"/>
                  <xs:element name="auflagenFreitext" minOccurs="0" type="dinspec91379:datatypeC"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element name="weisungen" minOccurs="0" maxOccurs="unbounded">
            <xs:complexType>
               <xs:sequence>
                  <xs:element name="weisungen" type="tns:Code.STRAF.Weisungen.Typ3"/>
                  <xs:element name="weisungenFreitext"
                              minOccurs="0"
                              type="dinspec91379:datatypeC"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element name="beginn" minOccurs="0" type="xs:date"/>
         <xs:element name="ende" minOccurs="0" type="xs:date"/>
         <xs:element name="rechtskraft"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="tns:Type.STRAF.Rechtskraft"/>
         <xs:element name="dauer" minOccurs="0" type="tns:Type.STRAF.Dauer"/>
         <xs:element name="vorbehalt" default="false" type="xs:boolean"/>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.Beweismittel">
      <xs:sequence>
         <xs:element name="nummer" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Da von anderen Elementen auf ein schon erfasstes Beweismittel verwiesen wird, ist eine eindeutige Nummer für das Element "Beweismittel" notwendig.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="art" minOccurs="0" type="tns:Code.STRAF.Beweismittel.Typ3">
            <xs:annotation>
               <xs:documentation>Dieses Element enthält die "Art" des Beweismittels. Mögliche Werte sind hier z.B. Zeuge, Sachverständiger, Beiakte, Einlassung, pol. Ermittlungsvermerk.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="aktenblatt" minOccurs="0" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Es ist die Blattzahl der Akte gemeint.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="kurzbezeichnung" minOccurs="0" type="dinspec91379:datatypeC"/>
         <xs:element name="inhalt" minOccurs="0" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Dieses Element steht für eine Art Inhaltsangabe des Beweismittels. Beispielsweise kann ein Beweismittel mit der Kurzbezeichnung Gutachten erfasst werden. Im Inhalt kann dazu dann die weitergehende Bewertung vorgenommen werden, z.B. "Gutachten zu den Einbruchspuren".</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="person"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="tns:Type.GDS.Ref.Rollennummer">
            <xs:annotation>
               <xs:documentation>Ist das Beweismittel eine Person (Zeuge, Sachverständiger), kann hier ein Verweis auf die Rollennummer eines Beteiligten im Grunddatensatz angegeben werden.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ref.asservate" minOccurs="0" type="dinspec91379:datatypeC"/>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.Dauer">
      <xs:annotation>
         <xs:documentation>Die Angabe einer zeitlichen Ausdehnung eines Ereignisses. Die Länge einer Zeitspanne bzw. Zeitraums.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="jahr" minOccurs="0" type="dinspec91379:datatypeC"/>
         <xs:element name="monate" minOccurs="0" type="dinspec91379:datatypeC"/>
         <xs:element name="wochen" minOccurs="0" type="dinspec91379:datatypeC"/>
         <xs:element name="tage" minOccurs="0" type="dinspec91379:datatypeC"/>
         <xs:element name="stunden" minOccurs="0" type="dinspec91379:datatypeC"/>
         <xs:element name="minuten" minOccurs="0" type="dinspec91379:datatypeC"/>
         <xs:element name="sekunden" minOccurs="0" type="dinspec91379:datatypeC"/>
         <xs:element name="sonstige" minOccurs="0" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Sonstige Angaben unter anderem z.B. Freizeitarrest für die Freizeiten</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="tagessatzhoehe" minOccurs="0" type="tns:Type.GDS.Geldbetrag"/>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.Entscheidung">
      <xs:sequence>
         <xs:element name="ref.entscheidungsdokument"
                     minOccurs="0"
                     type="tns:Type.GDS.Ref.SGO"/>
         <xs:element name="entscheidungsbehoerde">
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="tns:Type.GDS.Behoerde">
                     <xs:sequence>
                        <xs:element name="aktenzeichen" minOccurs="0" type="dinspec91379:datatypeC"/>
                     </xs:sequence>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="entscheidungsdatum" minOccurs="0" type="xs:date"/>
         <xs:element name="zustellung" minOccurs="0" maxOccurs="unbounded">
            <xs:complexType>
               <xs:sequence>
                  <xs:element name="zustellungsempfaenger" type="tns:Type.GDS.Ref.Rollennummer">
                     <xs:annotation>
                        <xs:documentation>Verweis auf Rollennummer</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="zustellungsdatum" minOccurs="0" type="xs:date"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element name="rechtskraft"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="tns:Type.STRAF.Rechtskraft"/>
         <xs:element name="entscheidungstenor"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="tns:Type.STRAF.Entscheidungstenor"/>
         <xs:element name="bezug" minOccurs="0" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Ein textueller Verweis auf die Entscheidung für interne Referenzierungen kann das Element Dokument/Verweis aus dem Grunddatensatz verwendet werden. Beispiel: Im Falle einer Berufung kann hier ein Verweis auf die ursprüngliche Entscheidung stehen.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.Entscheidungsart">
      <xs:sequence>
         <xs:element name="entscheidungsart" type="tns:Code.STRAF.Entscheidungsart.Typ3"/>
         <xs:element name="entscheidungsdatum" type="xs:date"/>
         <xs:element name="beschlussart"
                     minOccurs="0"
                     type="tns:Code.STRAF.Beschlussart.Typ3"/>
         <xs:element name="bescheidart"
                     minOccurs="0"
                     type="tns:Code.STRAF.Bescheidart.Typ3"/>
         <xs:element name="einstellungsart"
                     minOccurs="0"
                     type="tns:Code.STRAF.Einstellungsart.Typ3"/>
         <xs:element name="weisungen"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="tns:Code.STRAF.Weisungen.Typ3"/>
         <xs:element name="auflagen" minOccurs="0" maxOccurs="unbounded">
            <xs:complexType>
               <xs:sequence>
                  <xs:element name="auflage" type="tns:Code.STRAF.Auflagen.Typ3"/>
                  <xs:element name="betrag" minOccurs="0" type="tns:Type.GDS.Geldbetrag"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element name="rechtsfolgen" minOccurs="0">
            <xs:complexType>
               <xs:sequence>
                  <xs:element name="rechtsfolgenart"
                              minOccurs="0"
                              type="tns:Code.STRAF.Rechtsfolgen.Typ3"/>
                  <xs:element name="geldanordnungsart"
                              minOccurs="0"
                              type="tns:Code.STRAF.Geldanordnungsart.Typ3"/>
                  <xs:element name="betrag" minOccurs="0" type="tns:Type.GDS.Geldbetrag">
                     <xs:annotation>
                        <xs:documentation>Für die Angabe des Ordnungsgeldes und der Geldbuße</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="dauer" minOccurs="0" type="tns:Type.STRAF.Dauer">
                     <xs:annotation>
                        <xs:documentation>Hier können die Geldstrafe und die anderen Freiheitsentziehungen erfasst werden.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="rechtskraft" minOccurs="0" type="tns:Type.STRAF.Rechtskraft"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element name="bewaehrung" minOccurs="0" type="tns:Type.STRAF.Bewaehrung"/>
         <xs:element name="haft" minOccurs="0" maxOccurs="unbounded">
            <xs:complexType>
               <xs:sequence>
                  <xs:element name="haftart" minOccurs="0" type="tns:Code.STRAF.Haftart.Typ3"/>
                  <xs:element name="haftbeginn"
                              minOccurs="0"
                              type="tns:Code.STRAF.Haftbeginn.Typ3"/>
                  <xs:element name="beginn" minOccurs="0" type="xs:date"/>
                  <xs:element name="haftzeitende"
                              minOccurs="0"
                              type="tns:Code.STRAF.Haftzeitendeart.Typ3"/>
                  <xs:element name="ende" minOccurs="0" type="xs:date"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element name="bussgeldbescheid"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="tns:Type.STRAF.OWI.Bussgeldbescheid"/>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.Entscheidungstenor">
      <xs:annotation>
         <xs:documentation>Dieser Datentyp beinhaltet Angaben zu Beteiligten, Ergebnissen,. Anordnungen und OWI-Bereichen.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="betroffener"
                     maxOccurs="unbounded"
                     type="tns:Type.GDS.Ref.Rollennummer"/>
         <xs:element name="ergebnis"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="tns:Type.STRAF.Ergebnis">
            <xs:annotation>
               <xs:documentation>Für den Fall der Teilrechtskraft</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="wortlautEntscheidungstenor"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Entscheidungstenor im Wortlaut</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="anordnungsinhalt"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="tns:Type.STRAF.Anordnungsinhalt"/>
         <xs:element name="beweismittel"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="tns:Type.STRAF.Beweismittel"/>
         <xs:element name="asservate"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="tns:Type.STRAF.Asservate"/>
         <xs:element name="owi" minOccurs="0">
            <xs:annotation>
               <xs:documentation>Angaben, die in OWI Angelegenheit angeordnet werden bzw entschieden.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element name="tatangabenZusatztext"
                              minOccurs="0"
                              type="dinspec91379:datatypeC">
                     <xs:annotation>
                        <xs:documentation>Das sind weitere wichtige Erläuterungen zum Tatvorwurf, da nicht alle Tatumstände in den vorgegebenen Tatbestandsnummern beschrieben sind. Es können weitere Hinweise zum Tatvorwurf ausserhalb der geforderten Konkretisierungen darin vermerkt werden.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="bescheidZusatztext"
                              minOccurs="0"
                              type="dinspec91379:datatypeC">
                     <xs:annotation>
                        <xs:documentation>In dieses Feld können weitere Erläuterungen zum Bußgeldbescheid vermerkt werden. z.B. der Beweggrund, das Verfahren nicht einzustellen, oder Äusserungen zu Fragen des Betroffenen. Hier gibt es keine exakten Grenzen oder Beschränkungen.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="paragraf28aStVG" minOccurs="0" type="xs:boolean">
                     <xs:annotation>
                        <xs:documentation>Das Feld Paragraf_28a_StVG gibt an, ob aus wirtschaftlichen Gründen die Geldbuße reduziert wurde? Reduziert ? J/N</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="nebenfolgen" minOccurs="0" type="dinspec91379:datatypeC">
                     <xs:annotation>
                        <xs:documentation>Wenn ein Bußgeldbescheid verhängt wird, können damit sogenannte "Nebenfolgen" verbunden sein, also zusätzliche "Belastungen" für den Betroffenen. Bei Verkehrsordnungswidrigkeiten gibt es genau eine Nebenfolge:Fahrverbot.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="punkte" minOccurs="0" type="xs:integer">
                     <xs:annotation>
                        <xs:documentation>Die Anzahl der Punkte in Flensburg, die von der Bußgeldbehörde verhängt werden.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="abweichungRegelsatz" default="false" type="xs:boolean">
                     <xs:annotation>
                        <xs:documentation>Dieses Element erhält den Wert true, wenn die festgesetzte Sanktion vom Regelsatz des Bußgeldkatalogs abweicht (§ 17 OWiG).</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="absehenVonFahrverbot" default="false" type="xs:boolean">
                     <xs:annotation>
                        <xs:documentation>Dieses Element erhält den Wert true, wenn entgegen der Regel von einem Fahrverbot abgesehen worden ist (BKatV § 4 Abs. 4)</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="vollstreckbar" minOccurs="0" type="xs:boolean">
                     <xs:annotation>
                        <xs:documentation>Hinweis der Vollstreckbarkeit der Forderung</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="vollstreckungErfolglos" minOccurs="0" type="xs:boolean">
                     <xs:annotation>
                        <xs:documentation>Hinweis, dass Vollstreckungsmaßnahmen erfolglos waren.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="belehrung66Abs2Nr3OWiG" minOccurs="0" type="xs:boolean">
                     <xs:annotation>
                        <xs:documentation>Hinweis, dass eine Belehrung nach § 66 Abs. Nr. 3 OWiG stattgefunden hat.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.Ergebnis">
      <xs:annotation>
         <xs:documentation>Bezogen auf eine Tat kann hiermit ein Ergebnis aufgrund der Angabe einer Katalogkennziffer.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="ref.Tat" minOccurs="0" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Hier kann ein Verweis auf die entsprechende Tat angegeben werden.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ergebnisart"
                     minOccurs="0"
                     type="tns:Code.STRAF.Erledigungsarten.Typ3"/>
         <xs:element name="kennziffer"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="tns:Type.STRAF.Kennziffer">
            <xs:annotation>
               <xs:documentation>Da es derzeit keinen bundeseinheitlichen Kennziffernkatalog (ZSTV, AUMIAU) für Erledigungsarten gibt, besteht hier die Möglichkeit den jeweiligen Katalog mit entsprechender Kennziffer einzubinden.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.Erledigung">
      <xs:sequence>
         <xs:element name="art"
                     minOccurs="0"
                     type="tns:Code.STRAF.Erledigungsarten.Typ3">
            <xs:annotation>
               <xs:documentation>Für die Inhalte, die in diesem Element auftreten können, soll eine Codeliste verwendet werden (Zählkartenkennzeichen).</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="neuesAktenzeichen"
                     minOccurs="0"
                     type="tns:Type.GDS.Aktenzeichen">
            <xs:annotation>
               <xs:documentation>Möglichkeit der Mitteilung eines neuen Aktenzeichens bei interner Abgabe innerhalb der StA.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="erledigungsdatum" type="xs:date"/>
         <xs:element name="beteiligter"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="tns:Type.GDS.Ref.Rollennummer">
            <xs:annotation>
               <xs:documentation>Dieses Element enthält einen Verweis auf die Rollennummer eines Beteiligten im Grunddatensatz. Dadurch kann auch die Erledigung eines Verfahrens für einzelne Beschuldigte erfasst werden.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="verfahren"
                     minOccurs="0"
                     default="false"
                     type="xs:boolean">
            <xs:annotation>
               <xs:documentation>Mit diesem Element wird angegeben, ob die Erledigung das gesamte Verfahren betrifft. Wenn das Element den Wert false enthält, bezieht sich die Erledigung nur auf einen Teil des Verfahrens, z.B. auf einzelne Mitbeschuldigte oder einzelne Tatkomplexe.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ref.Tat"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Wenn sich die Erledigung nur auf einzelne Tatkomplexe bezieht, kann hier auf die erledigten Taten verwiesen werden.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="erledigungskennziffer"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="tns:Type.STRAF.Kennziffer"/>
         <xs:element name="erledigungsbezeichnung"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="dinspec91379:datatypeC"/>
         <xs:element name="betroffeneInstanz" minOccurs="0">
            <xs:annotation>
               <xs:documentation>Hier kann auf eine weitere Instanz verwiesen werden. Bei einer Erledigung durch Verbindung zu einem anderen Verfahren wird hier auf eine Instanz verwiesen, in der das führende AZ hinterlegt ist. Ist die Erledigung z.B. eine Abgabe, wird in der hier referenzierten Instanz die empfangende Behörde beschrieben.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element name="ref.instanznummer" type="dinspec91379:datatypeC">
                     <xs:annotation>
                        <xs:documentation>Dieses Element enthält einen Verweis auf die Instanz, bei der die oben angegebene ID verwendet wird. Verwiesen wird auf das Element Instanznummer.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.Fachdaten.Strafverfahren">
      <xs:sequence>
         <xs:element name="untervorgangsnummer"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Im Rahmen der Ermittlungen werden die einzelnen Vorgänge jeweils mit eigenen Vorgangsnummern geführt, die u.a. den Anzeigeerstattern bzw. Geschädigten bekannt gegeben werden. Nach Abgabe des Hauptvorgangs an die Staatsanwaltschaft und dortiger Erfassung als Ermittlungsverfahren ist hier nur die Vorgangsnummer des Hauptvorgangs bekannt. Wenn sich nun Anzeigeerstatter, Geschädigte, Versicherungen pp. mit einer Vorgangsnummer eines Untervorgangs an die Staatsanwaltschaft wenden, kann das Verfahren anhand dieser Nummer nicht ermittelt werden und muss bei der Polizei erfragt werden. Dabei kommt es immer wieder vor, dass die Staatsanwaltschaft mitteilt, dass der Vorgang noch bei der Polizei sei - die dann aber dem Betroffenen das Aktenzeichen der Staatanwaltschaft anhand der dortigen Verknüpfung des Unter- zum Hauptvorgang mitteilt. Das kann durch Übermittlung aller Vorgangsnummern der Untervorgänge verhindert werden.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="sachgebietsschluessel"
                     minOccurs="0"
                     type="tns:Code.STRAF.Sachgebietsschluessel.Typ3"/>
         <xs:element name="erledigung" minOccurs="0" type="tns:Type.STRAF.Erledigung"/>
         <xs:element name="einleitdatum" minOccurs="0" type="xs:date">
            <xs:annotation>
               <xs:documentation>Einleitdatum des Verfahrens bei der Polizei</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="eingangsdatumStA" minOccurs="0" type="xs:date"/>
         <xs:element name="personendaten"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="tns:Type.STRAF.Personendaten"/>
         <xs:element name="tat"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="tns:Type.STRAF.Tat">
            <xs:annotation>
               <xs:documentation>Einer Tat können beliebig viele Delikte zugewiesen werden. Einem Delikt wiederum können verschiedene durch den Grunddatensatz schon erfasste beteiligte Personen durch ihre Beteiligtennummern zugewiesen werden.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="tatmerkmal"
                     minOccurs="0"
                     type="tns:Code.STRAF.Tatmerkmal.Typ3"/>
         <xs:element name="haft" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
               <xs:documentation>Die Haftdaten eines Verfahrens sind in Bereiche unterteilt. "Ref_Dokument" verweist auf Entscheidungen, die einer Haft zugrunde liegen. Alle Daten, die sich auf den Haftaufenthalt beziehen, sind in dem Bereich "Haftvollzug" untergeordnet. Besucherserlaubnisse und Haftbeschränkungen, die z.B. bei U-Haft auftreten können (kein Kontakt zu Mitbeschuldigten), sind im Abschnitt "Haftkontrolle" untergebracht. Innerhalb dieses Elementes können beliebig viele Verweise, Haftaufenthalte (Haftvollzug) und beliebig viele Haftkontrollmaßahmen erfasst werden.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element name="haftvollzug" minOccurs="0" maxOccurs="unbounded">
                     <xs:annotation>
                        <xs:documentation>Hier ist jede Form der Inhaftierung gemeint.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element name="haftanstalt"
                                       minOccurs="0"
                                       type="tns:Code.GDS.Justizvollzug.Typ3"/>
                           <xs:element name="person" type="tns:Type.GDS.Ref.Rollennummer"/>
                           <xs:element name="beginn">
                              <xs:annotation>
                                 <xs:documentation>Beginn der Inhaftierung in der jeweiligen Sache</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:sequence>
                                    <xs:element name="datum" type="xs:date"/>
                                    <xs:element name="ort" minOccurs="0" type="dinspec91379:datatypeB"/>
                                    <xs:element name="uhrzeit" minOccurs="0" type="xs:time"/>
                                    <xs:element name="haftantritt" type="tns:Code.STRAF.Haftbeginn.Typ3">
                                       <xs:annotation>
                                          <xs:documentation>Für die Art des Haftbeginns kann eine Codeliste WL_Haftbeginn verwendet werden.</xs:documentation>
                                       </xs:annotation>
                                    </xs:element>
                                 </xs:sequence>
                              </xs:complexType>
                           </xs:element>
                           <xs:element name="ende" minOccurs="0">
                              <xs:annotation>
                                 <xs:documentation>Das Ende der Inhaftierung in der jeweiligen Sache. Das Enddatum stimmt nicht zwingend mit dem Entlassungsdatum überein. Der Gefangene kann z.B. nach dem Ende der einen Strafe noch eine weitere Strafe zu verbüßen haben.</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:sequence>
                                    <xs:element name="datum" type="xs:date"/>
                                    <xs:element name="Uhrzeit" minOccurs="0" type="xs:time"/>
                                    <xs:element name="beendigungsart"
                                                minOccurs="0"
                                                type="tns:Code.STRAF.Haftzeitendeart.Typ3">
                                       <xs:annotation>
                                          <xs:documentation>Die Beendigungsart des Haftvollzugs ist in einer Codeliste mit den Werte Entlassung, Flucht, Tod, Verlegung, Abschiebung angegeben.</xs:documentation>
                                       </xs:annotation>
                                    </xs:element>
                                 </xs:sequence>
                              </xs:complexType>
                           </xs:element>
                           <xs:element name="bemerkung" minOccurs="0" type="dinspec91379:datatypeC">
                              <xs:annotation>
                                 <xs:documentation>Weitere Angaben wie z.B. "Der/Die Verurteile(r) ist als Vorsatztäter zur Strafvollstreckung aufzunehmen" oder " Es besteht Selbstmordgefahr" oder "der Zweck der Vorführung".</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                           <xs:element name="haftart" type="tns:Code.STRAF.Haftart.Typ3"/>
                           <xs:element name="gefangenenbuchnummer"
                                       minOccurs="0"
                                       type="dinspec91379:datatypeC">
                              <xs:annotation>
                                 <xs:documentation>Die JVA verwaltet Gefangene unter dieser Nummer.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                           <xs:element name="haftdauer" minOccurs="0" type="tns:Type.STRAF.Dauer"/>
                           <xs:element name="ladungsdatum" minOccurs="0" type="xs:date"/>
                           <xs:element name="prueffrist" minOccurs="0">
                              <xs:annotation>
                                 <xs:documentation>Bereits absolvierte Termine zur Haftprüffrist tetc.</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:sequence>
                                    <xs:element name="vorschrift"
                                                minOccurs="0"
                                                type="tns:Code.STRAF.Pruefvorschrift.Typ3"/>
                                    <xs:element name="termin" minOccurs="0" type="xs:date">
                                       <xs:annotation>
                                          <xs:documentation>Termin, an dem die Prüfung stattgefunden hat</xs:documentation>
                                       </xs:annotation>
                                    </xs:element>
                                 </xs:sequence>
                              </xs:complexType>
                           </xs:element>
                           <xs:element name="abwesenheit" minOccurs="0" maxOccurs="unbounded">
                              <xs:annotation>
                                 <xs:documentation>Damit ist eine "Nicht-Anwesenheit" in der JVA gemeint, die nicht zu einer Haftunterbrechung führt.</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:sequence>
                                    <xs:element name="abwesenheitsart"
                                                minOccurs="0"
                                                type="tns:Code.STRAF.Abwesenheitsart.Typ3">
                                       <xs:annotation>
                                          <xs:documentation>Für die Art der Abwesenheit kann eine Codeliste mit möglichen Werten wie Urlaub, Ausgang,.. verwendet werden.</xs:documentation>
                                       </xs:annotation>
                                    </xs:element>
                                    <xs:element name="zeitraum" minOccurs="0">
                                       <xs:complexType>
                                          <xs:sequence>
                                             <xs:element name="von" minOccurs="0" type="dinspec91379:datatypeC"/>
                                             <xs:element name="bis" minOccurs="0" type="dinspec91379:datatypeC"/>
                                          </xs:sequence>
                                       </xs:complexType>
                                    </xs:element>
                                 </xs:sequence>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element name="haftkontrolle" minOccurs="0" maxOccurs="unbounded">
                     <xs:annotation>
                        <xs:documentation>Daten zur Haftkontrolle</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element name="besuchserlaubnis" minOccurs="0" maxOccurs="unbounded">
                              <xs:complexType>
                                 <xs:sequence>
                                    <xs:element name="besuchserlaubnisart"
                                                type="tns:Code.STRAF.Besuchserlaubnisart.Typ3">
                                       <xs:annotation>
                                          <xs:documentation>Für mögliche Werte, die hier auftreten können, ist eine Codeliste WL_Besuchserlaubnisart zu verwenden. Mögliche Werte sind hier z.B. Einzelsprecherlaubnis, Dauersprecherlaubnis</xs:documentation>
                                       </xs:annotation>
                                    </xs:element>
                                    <xs:element name="besucher" minOccurs="0" type="tns:Type.GDS.Ref.Rollennummer"/>
                                    <xs:element name="ausstellungsdatum" minOccurs="0" type="xs:date"/>
                                    <xs:element name="dauer" minOccurs="0" type="tns:Type.STRAF.Dauer"/>
                                 </xs:sequence>
                              </xs:complexType>
                           </xs:element>
                           <xs:element name="beschraenkung" minOccurs="0" type="dinspec91379:datatypeC">
                              <xs:annotation>
                                 <xs:documentation>Text z.B. Gemeinsame Unterbringung mit Mitbeschuldigten ist nicht zulässig.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element name="beweismittel"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="tns:Type.STRAF.Beweismittel"/>
         <xs:element name="strafanzeige" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
               <xs:documentation>Daten zur Angabe einer Strafanzeige im Unterschied zu einem Strafantrag</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element name="anzeigenerstatter"
                              minOccurs="0"
                              type="tns:Type.GDS.Ref.Rollennummer">
                     <xs:annotation>
                        <xs:documentation>Verweis auf einen Beteiligten, der als Anzeigeerstatter auftritt.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="anzeigedatum" minOccurs="0" type="xs:date">
                     <xs:annotation>
                        <xs:documentation>Das Datum der Anzeige.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="strafantragstellung" minOccurs="0" type="xs:boolean">
                     <xs:annotation>
                        <xs:documentation>Wurde Strafantrag gestellt? J/N</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="bescheidwunsch" minOccurs="0" type="xs:boolean">
                     <xs:annotation>
                        <xs:documentation>Wert, der angibt, ob vom Antragsteller ein Bescheid erwünscht wird? Ja/Nein</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element name="einspruchOWI"
                     minOccurs="0"
                     type="tns:Type.STRAF.OWI.Einspruch"/>
         <xs:element name="fahrzeug"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="tns:Type.STRAF.Fahrzeug"/>
         <xs:element name="untersuchung"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="tns:Type.STRAF.Untersuchung"/>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.Fachdaten.Verfahrensausgangsmitteilung">
      <xs:sequence>
         <xs:element name="erledigung"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="tns:Type.STRAF.Erledigung">
            <xs:annotation>
               <xs:documentation>Die Erledigungsdaten eines Verfahrens werden jetzt nicht mehr in einem globalen Element 'Erledigung' erfasst, sondern sind bei den Instanzdaten angesiedelt. Eine Art der Erledigung kann beispielsweise die Abgabe des Verfahrens an eine andere STA sein.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ergebnisse" minOccurs="0">
            <xs:complexType>
               <xs:sequence>
                  <xs:element name="rechtskraft" minOccurs="0" type="tns:Type.STRAF.Rechtskraft"/>
                  <xs:element name="entscheidung"
                              maxOccurs="unbounded"
                              type="tns:Type.STRAF.Entscheidungsart"/>
                  <xs:element name="rechtsmittel"
                              minOccurs="0"
                              maxOccurs="unbounded"
                              type="tns:Type.STRAF.Rechtsmittel"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.Fahrzeug">
      <xs:sequence>
         <xs:element name="nummer" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Da von anderen Elementen auf ein schon erfasstes Fahrzeug verwiesen wird, ist eine eindeutige Nummer für das Element "Fahrzeug" notwendig.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="art" type="tns:Code.STRAF.Fahrzeugart.Typ3"/>
         <xs:element name="unterart" minOccurs="0" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Gewichtsbezogene Angabe der Art des Fahrzeuges. Mögliche Werte sind hier z.B. KFZ mit Anhänger bis 2 t. zul.GG</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="antrieb"
                     minOccurs="0"
                     type="tns:Code.STRAF.Fahrzeugantrieb.Typ3">
            <xs:annotation>
               <xs:documentation>Dieses Textelement dient zur Beschreibung der Antriebsart. Mögliche Werte sind z.B. Otto- oder Dieselmotor.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="hersteller" minOccurs="0" type="dinspec91379:datatypeD"/>
         <xs:element name="typ" minOccurs="0" type="dinspec91379:datatypeC"/>
         <xs:element name="baujahr" minOccurs="0" type="dinspec91379:datatypeC"/>
         <xs:element name="erstzulassung" minOccurs="0" type="dinspec91379:datatypeC"/>
         <xs:element name="fahrgestellnummer" type="dinspec91379:datatypeC"/>
         <xs:element name="kennzeichen" minOccurs="0" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Amtliches Kennzeichen</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="landeskennzeichen"
                     minOccurs="0"
                     type="tns:Code.STRAF.KFZ.Kennzeichen.Typ3"/>
         <xs:element name="wert" minOccurs="0" type="dinspec91379:datatypeC"/>
         <xs:element name="personenbezug" minOccurs="0" maxOccurs="unbounded">
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="tns:Type.GDS.Ref.Rollennummer">
                     <xs:sequence>
                        <xs:element name="art"
                                    maxOccurs="unbounded"
                                    type="tns:Code.STRAF.Personenbezug.Typ3">
                           <xs:annotation>
                              <xs:documentation>Halter, Fahrer, Eigentümer...</xs:documentation>
                           </xs:annotation>
                        </xs:element>
                     </xs:sequence>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="ref.beweismittel" minOccurs="0" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Hier kann ein Verweis auf ein Beweismittel angegeben werden.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.Fuehrerschein">
      <xs:sequence>
         <xs:element name="fahrerlaubnisart"
                     minOccurs="0"
                     type="tns:Code.STRAF.Fahrerlaubnisart.Typ3">
            <xs:annotation>
               <xs:documentation>Für die Abbildung der Art eines Führerscheins wird eine Codeliste WL_Fahrerlaubnisart verwendet werden. Mögliche Werte sind z.B. Allgemeine Fahrerlaubnis (§ 5 StVZO)</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="klasse"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="tns:Code.STRAF.Fuehrerscheinklasse.Typ3">
            <xs:annotation>
               <xs:documentation>Für die Abbildung der Führerscheinklasse kann eine Codeliste WL_Fuehrerscheinklasse verwendet werden.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ausstellungsdatum" minOccurs="0" type="xs:date"/>
         <xs:element name="ausstellungsbehoerde"
                     minOccurs="0"
                     type="tns:Type.GDS.Behoerde"/>
         <xs:element name="fuehrerscheinnummer"
                     minOccurs="0"
                     type="dinspec91379:datatypeC"/>
         <xs:element name="abgabedatum" minOccurs="0" type="xs:date"/>
         <xs:element name="ablaufdatum" minOccurs="0" type="xs:date"/>
         <xs:element name="sicherstellungsdatum" minOccurs="0" type="xs:date"/>
         <xs:element name="rueckgabedatum" minOccurs="0" type="xs:date"/>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.Haftbefehl">
      <xs:sequence>
         <xs:element name="person" type="tns:Type.GDS.Ref.Rollennummer"/>
         <xs:element name="haftanstalt"
                     minOccurs="0"
                     type="tns:Code.GDS.Justizvollzug.Typ3"/>
         <xs:element name="haftart" type="tns:Code.STRAF.Haftart.Typ3"/>
         <xs:element name="vorfuehrung" minOccurs="0" type="xs:boolean"/>
         <xs:element name="datum" type="xs:date"/>
         <xs:element name="fuehrendesDelikt"
                     minOccurs="0"
                     type="tns:Type.STRAF.Kennziffer"/>
         <xs:element name="haftdauer" minOccurs="0" type="tns:Type.STRAF.Dauer"/>
         <xs:element name="gesamtkosten" minOccurs="0">
            <xs:complexType>
               <xs:sequence>
                  <xs:element name="gesamtbetrag" type="tns:Type.GDS.Geldbetrag"/>
                  <xs:element name="kosten" minOccurs="0" type="tns:Type.GDS.Geldbetrag"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.HyDaNe">
      <xs:sequence>
         <xs:element name="gds.ref.sgo" type="tns:Type.GDS.Ref.SGO"/>
         <xs:element name="hydane.herkunftDerDaten"
                     type="tns:Code.STRAF.Hydane.HerkunftDerDaten.Typ3"/>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.Kennziffer">
      <xs:annotation>
         <xs:documentation>Dieser Datentyp dient zur Abbildung einer Kennziffer oder Nummer eines benannten Katalogs und dessen Version.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="katalog" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Hier ist die Bezeichnung des Katalogs oder Verzeichnisses anzugeben, um klarzustellen, welcher Katalog für nachfolgende Kennziffer verwendet wird. Bsp: PKS, GKG, AUMIAU, ZSTV etc.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="version" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Welche konkrete Version des Katalogs oder Datum des Verzeichnisses liegt vor? z.B.. 1.4</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="wert" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Hier steht der konkrete Wert der Kennziffer aus dem bezeichneten Katalog z.B. 1110</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="zusatz" minOccurs="0" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Dieses Textelement dient zur Konkretisierung des Wertes und ist abhängig von dem verwendeten Katalog. Hier können auch Angaben gemacht werden, die als Zusatzinformation zu dem Wert gelten.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.Messung">
      <xs:annotation>
         <xs:documentation>Dieser Datentyp kann zur Angabe von Messergebnissen verwendet werden z.B. von Blutalkohol-Untersuchungen oder Geschwindigkeitsmessungen.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="messwert" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Der Wert der Messung z.B. 2,3</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="einheit" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>z.B. km/h, Tonnen, Promille</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="gegenstand" minOccurs="0" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Eine Beschreibung der gemessenen Größe bzw. der Gegenstand der Messung z.B. Geschwindigkeit, Gewicht, Alkohol, ...</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.OWI.Bussgeldbescheid">
      <xs:sequence>
         <xs:element name="erlassdatum" minOccurs="0" type="tns:Type.GDS.Datumsangabe"/>
         <xs:element name="rechtskraft" minOccurs="0" type="tns:Type.STRAF.Rechtskraft"/>
         <xs:element name="bussgeldverjaehrung"
                     minOccurs="0"
                     type="tns:Type.GDS.Datumsangabe"/>
         <xs:element name="geldbusse" minOccurs="0" type="xs:double"/>
         <xs:element name="teilzahlungGeldbusseGesamt" minOccurs="0" type="xs:double"/>
         <xs:element name="teilzahlungEinzeln"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="xs:double"/>
         <xs:element name="auslagen" minOccurs="0" type="xs:double"/>
         <xs:element name="teilzahlungAuslagen"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="xs:double"/>
         <xs:element name="kasse" minOccurs="0">
            <xs:complexType>
               <xs:sequence>
                  <xs:element name="ref.beteiligtennummer" type="dinspec91379:datatypeC"/>
                  <xs:element name="kassenzeichen" minOccurs="0" type="dinspec91379:datatypeC"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element name="tat" minOccurs="0" type="tns:Type.STRAF.OWI.Tat"/>
         <xs:element name="vollzugsbehoerde"
                     minOccurs="0"
                     type="tns:Type.STRAF.OWI.Vollzugsbehoerde"/>
         <xs:element name="bussgeldkatalog"
                     minOccurs="0"
                     type="tns:Type.STRAF.OWI.Bussgeldkatalog">
            <xs:annotation>
               <xs:documentation>Der Bußgeldkatalog enthält Elemente zur Abbildung von OWI-Tatbeständen und deren Einordnung in den Bußgeldkatalog.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.OWI.Bussgeldkatalog">
      <xs:sequence>
         <xs:element name="auswahl_tatbestandsnummer.text">
            <xs:complexType>
               <xs:choice>
                  <xs:element name="tatbestandsnummer" type="tns:Type.STRAF.Kennziffer">
                     <xs:annotation>
                        <xs:documentation>Hier ist die Tatbestandsnummer aus dem bezeichneten Bußgeldkatalog anzugeben wie auch die Tabellennummer im Zusatz.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="text" type="dinspec91379:datatypeC">
                     <xs:annotation>
                        <xs:documentation>Beschreibung des Delikts mittels Freitext, falls dieses nicht über den Tatbestandskatalog abgedeckt werden kann.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:choice>
            </xs:complexType>
         </xs:element>
         <xs:element name="textalternative" minOccurs="0" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Bestimmte Tatbestandsnummern erfordern die zusätzliche Angabe einer Alternative.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="konkretisierung" minOccurs="0" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Bestimmte Tatbestandsnummern enthalten an einer oder mehreren Stellen Alternativen oder die Möglichkeit zum Einfügen von zusätzlichem Text. Für die Art und Weise, wie diese Konkretisierung anzugeben ist, gibt es eingehende Regeln. Die Einhaltung dieser Regeln werden mit XML-Mitteln nicht überprüft.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="gemessenerWert" minOccurs="0" type="tns:Type.STRAF.Messung">
            <xs:annotation>
               <xs:documentation>Bestimmte Tatbestände erfordern die Angabe eines gemessenen Wertes.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="zulaessigerWert" minOccurs="0" type="tns:Type.STRAF.Messung">
            <xs:annotation>
               <xs:documentation>Bestimmte Tatbestände erfordern die Angabe eines zulässigen Wertes.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="differenz" minOccurs="0" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Bestimmte Tatbestände erfordern die Angabe einer Differenz von gemessenem und zulässigem Wert.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="vorsatz" minOccurs="0" type="xs:boolean">
            <xs:annotation>
               <xs:documentation>Handelt es sich um eine vorsätzliche Tat? J/N</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="fahrverbot" minOccurs="0" type="tns:Type.STRAF.Dauer">
            <xs:annotation>
               <xs:documentation>Angabe der vorgesehenen Dauer des Fahrverbots, die laut Bussgeldkatalog anzusetzen ist. (z.B. 6 Monate)</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="punkte" minOccurs="0" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Die Flensburgpunkte, die laut Bussgeldkatalog anzuordnen sind.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="geldbusse" minOccurs="0" type="dinspec91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Die Wertangabe (auch Grenzangaben) der Geldbuße, die laut Bussgeldkatalog vorgesehen ist.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:choice minOccurs="0">
            <xs:element name="tateinheit">
               <xs:complexType>
                  <xs:sequence>
                     <xs:element name="ref.delikt" minOccurs="0" type="dinspec91379:datatypeC"/>
                     <xs:element name="beschreibung" minOccurs="0" type="dinspec91379:datatypeC"/>
                  </xs:sequence>
               </xs:complexType>
            </xs:element>
            <xs:element name="tatmehrheit">
               <xs:complexType>
                  <xs:sequence>
                     <xs:element name="ref.delikt" minOccurs="0" type="dinspec91379:datatypeC"/>
                     <xs:element name="beschreibung" minOccurs="0" type="dinspec91379:datatypeC"/>
                  </xs:sequence>
               </xs:complexType>
            </xs:element>
         </xs:choice>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.OWI.Einspruch">
      <xs:sequence>
         <xs:element name="datumDesEinspruchs" type="xs:date"/>
         <xs:element name="bussgeldbehoerde">
            <xs:complexType>
               <xs:sequence>
                  <xs:element name="ref.instanznummer" type="dinspec91379:datatypeC"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element name="bussgeldbescheiddatum" type="xs:date"/>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.STRAF.OWI.Erledigungsmitteilung">
      <xs:sequence>
         <xs:element name="erledigungsart"
                     minOccurs="0"
                     type="tns:Code.STRAF.OWI.Erledigungsart.Typ3"/>
         <xs:element name="dauer" minOccurs="0" type="tns:Type.STRAF.Dauer"/>
         <xs:element name="erledigungsdatum"
