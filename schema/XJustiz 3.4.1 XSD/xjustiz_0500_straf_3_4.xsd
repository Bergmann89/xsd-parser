                                                      <xs:element name="glaette" type="dinspec91379:datatypeC"/>
                                                      <xs:element name="naesse" type="dinspec91379:datatypeC"/>
                                                   </xs:choice>
                                                </xs:complexType>
                                             </xs:element>
                                          </xs:sequence>
                                       </xs:complexType>
                                    </xs:element>
                                 </xs:sequence>
                              </xs:complexType>
                           </xs:element>
                           <xs:element name="teilzahlung" minOccurs="0">
                              <xs:complexType>
                                 <xs:sequence>
                                    <xs:element name="betrag"
                                                minOccurs="0"
                                                maxOccurs="unbounded"
                                                type="tns:Type.GDS.Geldbetrag"/>
                                    <xs:element name="neueHaftdauer" minOccurs="0" type="tns:Type.STRAF.Dauer"/>
                                 </xs:sequence>
                              </xs:complexType>
                           </xs:element>
                           <xs:element name="haftbefehl" minOccurs="0" type="tns:Type.STRAF.Haftbefehl"/>
                           <xs:element name="beschlagnahme"
